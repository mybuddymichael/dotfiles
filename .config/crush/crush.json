{
	"$schema": "https://charm.land/crush.json",
	"lsp": {
		"go": {
			"command": "gopls"
		},
		"typescript": {
			"command": "typescript-language-server",
			"args": ["--stdio"]
		}
	},
	"providers": {
		"ollama": {
			"name": "Ollama",
			"base_url": "http://localhost:11434/v1/",
			"type": "openai",
			"models": [
				{
					"name": "GPT-OSS 20b",
					"id": "gpt-oss:20b",
					"cost_per_1m_in": 0,
					"cost_per_1m_out": 0,
					"cost_per_1m_in_cached": 0,
					"cost_per_1m_out_cached": 0,
					"context_window": 128000,
					"default_max_tokens": 20000,
					"can_reason": false,
					"supports_attachments": false,
					"has_reasoning_efforts": false
				}
			]
		},
		"Claude Code": {
			"api_key": "Bearer $(bunx anthropic-api-key)",
			"base_url": "https://api.anthropic.com/v1",
			"disable": false,
			"extra_headers": {
				"anthropic-beta": "oauth-2025-04-20",
				"anthropic-version": "2023-06-01"
			},
			"models": [
				{
					"can_reason": true,
					"context_window": 200000,
					"cost_per_1m_in": 15,
					"cost_per_1m_in_cached": 1.5,
					"cost_per_1m_out": 75,
					"cost_per_1m_out_cached": 75,
					"default_max_tokens": 32000,
					"has_reasoning_efforts": true,
					"id": "claude-opus-4-1-20250805",
					"name": "Claude Opus 4.1",
					"supports_attachments": true
				},
				{
					"can_reason": true,
					"context_window": 200000,
					"cost_per_1m_in": 3,
					"cost_per_1m_in_cached": 0.3,
					"cost_per_1m_out": 15,
					"cost_per_1m_out_cached": 15,
					"default_max_tokens": 64000,
					"has_reasoning_efforts": false,
					"id": "claude-sonnet-4-20250514",
					"name": "Claude Sonnet 4",
					"supports_attachments": true
				}
			],
			"name": "Claude Code",
			"system_prompt_prefix": "You are Claude Code, Anthropic's official CLI for Claude.",
			"type": "anthropic"
		}
	}
}
