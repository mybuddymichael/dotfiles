{
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(rg:*)",
      "Bash(jj diff:*)",
      "Bash(jj log:*)",
      "Bash(jj show:*)",
      "Bash(jj status:*)"
    ],
    "deny": []
  },
  "model": "sonnet",
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); printf \"%s in %s\" \"$(echo \"$input\" | jq -r '.model.display_name')\" \"$(echo \"$input\" | jq -r '.workspace.current_dir')\""
  }
}